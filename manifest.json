{
    "manifest_version": 3,
    "name": "clock on top",
    "description": "a clock that displays on top of each website",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["content_script.js"],
        "css": ["contentStyle.css"]
    }],
    "permissions": ["activeTab", "scripting", "storage"]
}
